Welcome to java program using TestNG
public class FirstClass {
	String s;
	String m;
	int a;
	int b;
	int c;
@Test
	public void testCase1() {
		System.out.println("FirstClass1"+" "+Thread.currentThread().getId());
		int a=10;
		int b=20;
		//c=a < b? a:b;
		//System.out.println(c);
		Assert.assertTrue(a<b);
		System.out.println("Hello");
	}
	@Test
	public void testCase2() {
		System.out.println("FirstClass2"+" "+Thread.currentThread().getId());
	//s="Suganya";
	m="Mohana";
	Assert.assertEquals("Mohana", m);
	System.out.println("Thank you for processing");
	}
	}
